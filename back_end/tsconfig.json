{
    "compilerOptions": {
        "target": "es2016",
        "lib": [
            "es6"
        ],
        "module": "commonjs",
        "rootDir": "src",
        "resolveJsonModule": true,
        "allowJs": true,
        "outDir": "dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "skipLibCheck": true,
    },
    "include": [
        "src/**/*.ts",
    ]
}